{
    "plugins": [
        "hide-element",
        "back-to-top-button",
        "chapter-fold",
        "code",
        "splitter",
        "-lunr",
        "-search",
        "search-pro",
        "insert-logo",
        "tbfed-pagefooter",
        "popup",
        "github",
        "katex",
        "github-buttons",
        "anchors",
        "anchor-navigation-ex",
        "todo",
        "klipse",
        "donate"
